<AnchorPane fx:id="mainPane" prefHeight="311.0" prefWidth="600.0" stylesheets="@../styles/application.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ke.co.sema_atsm.UI.ViewControllers.ConfirmShutDownControllers">
