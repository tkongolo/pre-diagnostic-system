         <children>
            <VBox alignment="CENTER" prefHeight="277.0" prefWidth="520.0">
               <children>
                  <StackPane prefHeight="84.0" prefWidth="520.0">
                     <children>
                        <Label fx:id="titleLabel" text="WRONG PASSWORD" textFill="#d71616" visible="false">
                           <font>
                              <Font size="18.0" />
                           </font>
                        </Label>
                     </children>
                  </StackPane>
                  <StackPane prefHeight="59.0" prefWidth="520.0">
                     <children>
                        <PasswordField fx:id="pField" alignment="CENTER" promptText="ENTER PASSWORD">
