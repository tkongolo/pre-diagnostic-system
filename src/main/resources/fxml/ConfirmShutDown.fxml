                              </StackPane>
                              <StackPane prefHeight="86.0" prefWidth="136.0">
                                 <children>
                                    <Button fx:id="cancelButton" mnemonicParsing="false" onAction="#cancelButtonClick" styleClass="confpassCancelButton" text="CANCEL" />
                                 </children>
                              </StackPane>
                           </children>
                        </HBox>
                     </children>
                  </StackPane>
               </children>
            </VBox>
         </children>
         <opaqueInsets>
            <Insets />
